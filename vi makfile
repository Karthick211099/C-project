outfile: project.o stud_add.o stud_show.o stud_del.o stud_dele_all.o stud_revlist.o stud_count.o stud_save.o stud_sort.o stud_modify.o print.o
        cc project.c stud_add.c stud_show.c stud_del.c stud_dele_all.c stud_revlist.c stud_count.c stud_save.c stud_modify.c stud_sort.c print.c -o outfile

project.o: project.c
        cc -c project.c
stud_add.o: stud_add.c
        cc -c stud_add.c
stud_show.o: stud_show.c
        cc -c stud_show.c
stud_del.o: stud_del.c
        cc -c stud_del.c
stud_dele_all.o: stud_dele_all.c
        cc -c stud_dele_all.c
stud_revlist.o: stud_revlist.c
        cc -c stud_revlist.c
stud_count.o: stud_count.c
        cc -c stud_count.c
stud_save.o: stud_save.c
        cc -c stud_save.c
stud_modify.o: stud_modify.c
        cc -c stud_modify.c
stud_sort.o: stud_sort.c
        cc -c stud_sort.c
print.o: print.c
        cc -c print.c